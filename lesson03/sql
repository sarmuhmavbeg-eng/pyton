-- ## Primary key, Foreign key  

каталог окпо праймерики
превичкадаги окпо форейж

create database stat03_group2

use stat03_group2

create table Katalog (OKPO varchar(30),INN varchar (30), name varchar (100))

alter table Katalog

alter column OKPO varchar(30) not null



alter table Katalog
add constraint PK_OKPO primary key (OKPO)

create table Pervichka (OKPO varchar(30) foreign key references Katalog (OKPO),INN varchar(30),Hisobot_oyi int) 

insert into Katalog values (1234567,987654321,'AGMK')

insert into Pervichka values (1234567,987654321,1000)

select * from Katalog
select * from Pervichka



